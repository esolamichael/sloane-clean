# Environment variables example for the Sloane frontend
# Copy this file to .env and add your actual values

# Google Maps API key (required for business search)
REACT_APP_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Base API URL for local development
REACT_APP_API_BASE_URL=http://localhost:5000/api

# Feature flags
REACT_APP_ENABLE_DEBUG_LOGGING=true